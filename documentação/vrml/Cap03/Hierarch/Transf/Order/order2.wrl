#VRML V2.0 utf8
# Exemplo de ordem das transformações:
# translação e rotação (notar que o Transform mais proximo
#                       do objeto acontece primeiro)
Inline { url "../old_tcoord.wrl" }
Transform {
	rotation 0 0 1 0.8
	children
		Transform {
			translation 2 1 0
			children [
				Inline { url "../coord.wrl" },
				Inline { url "../obj.wrl" }
			]
		}
}
