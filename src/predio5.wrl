Transform {
	translation 0 5 0
	children [
		Shape {
			 appearance Appearance {
               texture ImageTexture {
                   repeatS TRUE
                   repeatT TRUE
                   url "textures/predio5.jpg"
                 }
               textureTransform TextureTransform {
                   scale 1 3
               }
            }

			geometry Box {
				size 7 10.2 10
			}
		}

		Transform {
			translation 0 0 5.15
			children [
				DEF Quadradinho Shape {
					appearance Appearance {
                       texture ImageTexture {
                           repeatS TRUE
                           repeatT TRUE
                           url "textures/concreto2.jpg"
                         }
                       textureTransform TextureTransform {
                           scale 1 3
                       }
                    }

					geometry Box {
						size 2 10 .3
					}
				}
			]
		}

		Transform {
			rotation 0 1 0 1.57
			translation 3.6 0 2.5
			children [
				USE Quadradinho
			]
		}


		Transform {
			translation 2 0 4
			children [
				Shape {
                    appearance Appearance {
                       texture ImageTexture {
                           repeatS TRUE
                           repeatT TRUE
                           url "textures/concreto2.jpg"
                         }
                       textureTransform TextureTransform {
                           scale 1 3
                       }
                    }

					geometry Cylinder {
						height 10
						radius 2
					}
				}
			]
		}
	]
}