Transform {
	translation 0 5 0
	children [
		Shape {
			appearance Appearance {
					texture ImageTexture {
                        repeatS TRUE
                        repeatT TRUE
                        url "textures/predio2.jpg"
                    }
                    textureTransform TextureTransform {
                         scale 1 5
                    }
			}
			geometry Box { size 5 10 5 }
	  	}

	  	Transform {
	  		translation 2.5 0 2.5
	  		children [
			  	DEF bordaRedondaPredioMaior Shape {
			  		appearance Appearance {
						texture ImageTexture {
                            repeatS TRUE
                            repeatT TRUE
                            url "textures/concreto.jpg"
                        }
                        textureTransform TextureTransform {
                             scale 2 10
                        }
					}
					geometry Cylinder {
						height 10
						radius .5
					}			
			  	}
			]
		}

		Transform {
	  		translation 2.5 0 -2.5
	  		children [
			  	USE bordaRedondaPredioMaior
			]
		}

		Transform {
	  		translation -2.5 0 2.5
	  		children [
			  	USE bordaRedondaPredioMaior
			]
		}

		Transform {
	  		translation -2.5 0 -2.5
	  		children [
			  	USE bordaRedondaPredioMaior
			]
		}

	  	Transform {
	  		translation 0 6 0
	  		children [
	  			Shape {
					appearance Appearance {
						texture ImageTexture {
                            repeatS TRUE
                            repeatT TRUE
                            url "textures/predio2.jpg"
                        }
                        textureTransform TextureTransform {
                             scale 1 2
                        }
					}
					geometry Box { size 3.5 4 3.5 }			
	  			}
	  			
	  			Transform {
			  		translation 1.9 0 1.9
			  		children [
					  	DEF bordaRedondaPredioMenor Shape {
					  		appearance Appearance {
								texture ImageTexture {
                                    repeatS TRUE
                                    repeatT TRUE
                                    url "textures/concreto.jpg"
                                }
                                textureTransform TextureTransform {
                                     scale 2 4
                                }
							}
							geometry Cylinder {
								height 4
								radius .5
							}			
					  	}
					]
				}

				Transform {
			  		translation 1.9 0 -1.9
			  		children [
					  	USE bordaRedondaPredioMenor
					]
				}

				Transform {
			  		translation -1.9 0 1.9
			  		children [
					  	USE bordaRedondaPredioMenor
					]
				}

				Transform {
			  		translation -1.9 0 -1.9
			  		children [
					  	USE bordaRedondaPredioMenor
					]
				}
	  		]
	  	}

	  	

  	]
}